<h2>Search Books</h2>

<form action="/books" method="GET">
    <div>
        <label>Book Name or Title</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
    <div>
        <label>Published After</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>




    <button type="submit">Search Book</button>
</form>

<br>
<% books.forEach(book => { %>
    <div><%= book.name %> </div>
<% }) %> 



